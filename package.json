{
  "name": "sport-diary-server",
  "version": "1.0.0",
  "description": "A platform  where trainers and participants exchange and share information as well they can track their progress on the assigned training program/s.",
  "scripts": {
    "dev:watch": "cross-env NODE_ENV=development ts-node ./src/server/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:prod": "npm-run-all clean:dist build:client build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack.prod.client.ts",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack.prod.server.ts",
    "clean:dist": "rm -rf dist",
    "start:prod": "cross-env NODE_ENV=production nodemon ./dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boyskila/sport-diary-server.git"
  },
  "keywords": [
    "sport",
    "diary",
    "training",
    "trainers",
    "progress"
  ],
  "author": "boyskila",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/boyskila/sport-diary-server/issues"
  },
  "homepage": "https://github.com/boyskila/sport-diary-server#readme",
  "devDependencies": {
